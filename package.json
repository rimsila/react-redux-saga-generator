    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
