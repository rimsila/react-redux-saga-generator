{
  "<========================= APP INFO ==================================>": "",
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Sila",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimsila/react-redux-saga-generator.git"
  },
  "bugs": {
    "url": "https://github.com/rimsila/react-redux-saga-generator.git/issues"
  },
  "homepage": "https://github.com/rimsila/react-redux-saga-generator.git",
  "<========================= SCRIPT ==================================>": "",
  "scripts": {
    "------------------ HELPERS ----------------------------------------": "",
    "clean": "rimraf build/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "------------------ DEVELOPMENT -------------------------------------": "",
    "start": "yarn dev",
    "https:dev": "webpack-dev-server --https --config=configs/webpack/dev.js",
    "stag:dev": "webpack-dev-server --config=configs/webpack/stag.dev",
    "dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "------------------ PRODUCTION ---------------------------------------": "",
    "build": "yarn clean && webpack -p --config=configs/webpack/prod.js",
    "https:prod": "webpack -p --https --config=configs/webpack/prod.js",
    "prod": "node express.js",
    "analyze": "yarn build && webpack -p --config=configs/webpack/analyzer.js",
    "deploy": "gh-pages -d build",

    "------------------ TESTING ------------------------------------------": "",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "------------------ TOOL & GENERATOR ---------------------------------": "",
    "store": "cd ./src && yo nextjs-typescript-antd:store",
    "action": "cd ./src && yo nextjs-typescript-antd:action",
    "page": "cd ./src && yo nextjs-typescript-antd:page",
    "com": "cd ./src && yo nextjs-typescript-antd:com",
    "enum": "cd ./src && yo nextjs-typescript-antd:enum",
    "hoc": "cd ./src && yo nextjs-typescript-antd:hoc",
    "hook": "cd ./src && yo nextjs-typescript-antd:hook",
    "context": "cd ./src && yo nextjs-typescript-antd:context",
    "model": "cd ./src && yo nextjs-typescript-antd:model"
  },
  "<========================= DEPENDENCY ==================================>": "",
  "dependencies": {
    "axios": "^0.20.0",
    "cors": "^2.8.5",
    "gh-pages": "^3.1.0",
    "mini-css-extract-plugin": "^1.0.0",
    "react": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "<========================= CONFIG ==================================>": "",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "<========================= DEV_DEPENDENCY ==================================>": "",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "d": "^1.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-redux-saga": "^1.2.1",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
